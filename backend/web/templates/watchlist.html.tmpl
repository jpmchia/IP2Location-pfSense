{{define "watchlist.html.tmpl"}}
<div class="container">
    <div class="row">
        <div class="col-md-12">
            <h3>Template</h3>
            <p>This is my templated content.</p>
            <p class="card-text">Message: {{.message }}</p>
            <p class="card-text">IP Address: {{.IPAddr }}</p>
            <h1>{{.Title}}</h1>
            <span>{{.IPAddr}}</span>
            <span>{{.RealIP}}</span>
            <span>{{.APIKey}}</span>
            <span>{{.APIKeyExpires}}</span>
            <span>{{.Message}}</span>
        </div>
    </div>
</div>
{{end}}